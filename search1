package SEARCH;
import org.openqa.selenium.WebDriver;
import org.openqa.selenium.Keys;
import org.openqa.selenium.chrome.ChromeDriver;
//import org.openqa.selenium.support.ui.Select;
import org.openqa.selenium.By;
public class search {
	 public static void main(String[] args) { 
			System.setProperty("webdriver.chrome.driver","C:\\selenium driver\\chromedriver");
		    String baseURL = "https:www.accuweather.com";
		    WebDriver driver = new ChromeDriver();
			driver.get(baseURL);
			driver.findElement(By.xpath("/html/body/div[2]/div[1]/div[1mail.com");
	        driver.findElement(By.xpath("/html/body/div[2]/div[1]/div[1mail.com")).sendKeys(Keys.RETURN);
            driver.quit();
                
  }
}
